{
  "name": "reminder-presensi-bps",
  "version": "2.0.0",
  "description": "WhatsApp-based reminder for BPS Kalimantan Utara attendance with enterprise features",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test:pagi": "node src/index.js --time 07:24",
    "test:sore": "node src/index.js --time 16:04",
    "test:sore:jumat": "node src/index.js --time 16:34 --day 5",
    "test:fast": "node src/index.js --time 07:20 --speed 60"
  },
  "keywords": [
    "whatsapp",
    "reminder",
    "presensi",
    "bps",
    "attendance",
    "kalimantan-utara"
  ],
  "author": "BPS Provinsi Kalimantan Utara",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "node-cron": "^4.2.1",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.34.6"
  }
}
